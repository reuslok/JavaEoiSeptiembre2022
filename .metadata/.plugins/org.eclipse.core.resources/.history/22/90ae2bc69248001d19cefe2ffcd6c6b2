package cajeroautomatico;

public class CajeroAutomatico {
	

	public static boolean initializated=false;
	public static Titular[] titulares=new Titular[3];
	public static void main(String[] args) 	{	
	
		if(!initializated) 
			initializated=InitData();	
		
	}
	
	public static boolean InitData() { 
		titulares= {
				new Titular("pepe", "paco", "adkawjdja"),
				new Titular("maria","mari","123qasdj"),
				new Titular("mario", "maro", "dawdasdf")
			};
		
			Cuenta[] cuentas = {
				new Cuenta(titulares[0],1000.0*2),
				new Cuenta(titulares[1],1000.0*3),
				new Cuenta(titulares[2],1000.0*4)
				
				};
			
			return true;
		}
	}
